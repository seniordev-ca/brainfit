  "scripts": {
    "lint-all": "eslint . --ignore-path .eslintignore --max-warnings=0 --cache --ext .js,.jsx,.ts,.tsx",
    "lint-all-fix": "eslint . --ignore-path .eslintignore --max-warnings=0 --cache --ext .js,.jsx,.ts,.tsx --fix",
    "audit-server": "npx audit-ci",
    "audit-client": "npx audit-ci ./react",
    "audit-all": "npm run audit-server && npm run audit-client",
    "test-unit-client": "cd react && npm run test-client",
    "test-unit-server": "jest",
    "test-int-client": "echo 0",
    "test-int-server": "echo 0",
    "test-unit": "npm run test-unit-client && npm run test-unit-server",
    "test-int": "npm run test-int-client && npm run test-int-server",
    "test-all": "npm run test-unit && npm run test-int",
    "test": "npm run test-unit",
    "test-coverage": "jest --collectCoverage",
    "export-uml": "./scripts/export-uml-diagrams.sh",
    "debug:windows": "SET SLS_DEBUG=* && node --inspect node_modules\\serverless\\bin\\serverless offline  --config serverless.dev.yml",
    "debug:linux": "export SLS_DEBUG=* && node --inspect /usr/local/bin/serverless offline  --config serverless.dev.yml",
    "debug": "export SLS_DEBUG=* && node --inspect=127.0.0.1:27017 node_modules/serverless/bin/serverless offline --config serverless.dev.yml",
    "debug:watch": "./scripts/serverlessTesting.sh",
    "debug:watch-windows": ".\\scripts\\serverlessTesting.bat",
    "prepare": "husky install"
  },
