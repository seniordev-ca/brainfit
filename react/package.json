{
  "name": "mindovermatter-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true craco start",
    "dev": "cross-env REACT_APP_API_URL=http://localhost:4000/local craco start",
    "dev:ionic": "ionic serve -l",
    "ionic:build": "craco build",
    "ionic:serve": "craco start",
    "dev-windows": "set PORT=4001 && set REACT_APP_API_URL='http://localhost:4000/local' && craco start",
    "build": "craco build",
    "build-sync": "npm run build && npx cap sync",
    "build-local-android": "cross-env REACT_APP_API_URL=http://10.0.2.2:4000/local craco build",
    "build-client": "DISABLE_ESLINT_PLUGIN=true SKIP_PREFLIGHT_CHECK=true ./node_modules/@craco/craco/bin/craco.js build",
    "test": "npx craco test --watchAll=false",
    "test-coverage": "npx craco test --watchAll=false --collectCoverage",
    "test-client": "cross-env SKIP_PREFLIGHT_CHECK=true ./node_modules/@craco/craco/bin/craco.js test --watchAll=false --maxWorkers=20%",
    "test:watch": "craco test --watchAll=true",
    "update-snapshots": "react-scripts test --no-cache --updateSnapshot",
    "eject": "react-scripts eject",
    "storybook": "cross-env DISABLE_ESLINT_PLUGIN=true NODE_PATH=src start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint . --ext .tsx,.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
